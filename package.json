{
  "author": "Robert Smieja",
  "dependencies": {
    "@mdx-js/mdx": "^1.6.18",
    "@mdx-js/react": "^1.6.18",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "gatsby": "^2.24.72",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-manifest": "^2.4.34",
    "gatsby-plugin-mdx": "^1.2.45",
    "gatsby-plugin-offline": "^3.2.31",
    "gatsby-plugin-react-helmet": "^3.3.13",
    "gatsby-plugin-sharp": "^2.6.39",
    "gatsby-plugin-typescript": "^2.4.21",
    "gatsby-source-filesystem": "^2.3.34",
    "gatsby-transformer-sharp": "^2.5.17",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0"
  },
  "description": "Template repository for a Typescript-based Node.js project",
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.7",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.5.2",
    "lint-staged": "^10.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  },
  "keywords": [
    "gatsby",
    "typescript",
    "nodejs",
    "mdx",
    "mdxjs",
    "template"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,md}": [
      "prettier --write"
    ],
    "src/*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ]
  },
  "name": "gatsby-starter-mdx-typescript",
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "npm run lint:fix",
    "lint": "eslint . --ext .js,.json,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.json,.jsx,.ts,.tsx  --fix",
    "lint:staged": "lint-staged",
    "prettier": "prettier --write '*.{js,ts,jsx,tsx,json,md}'",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "version": "npm run lint:fix && git add -A ."
  },
  "version": "0.0.1"
}
